.container .moreFun text {
  padding: 0 90rpx;
}

.container .moreFun .iconfont {
  font-size: 45rpx;
}


.container .finish .header {
  padding-left: 30rpx;
  height: 70rpx;
  line-height: 70rpx;
  color: #b6b6b6;
  font-size: 35rpx;
  font-weight: 800;
  background-color: #fff;
  border-radius: 10rpx;
}

.container .finish .finish_list {
  margin-top: 30rpx;
}

.container .finish .header text {
  font-size: 30rpx;
}

.container .finish .header .length {
  float: right;
}

.container .finish .header .icoBefore {
  float: right;
  transform: rotate(0deg);
  transition: transform 0.5s;
}

.container .finish .header .icoAfter {
  float: right;
  transform: rotate(-90deg);
  transition: transform 0.5s;
}

