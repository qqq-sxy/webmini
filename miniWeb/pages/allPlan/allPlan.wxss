.container .finish .listUl .listLi {
  /* padding-bottom: 70rpx; */
  margin-bottom: 20rpx;
  border-radius: 10rpx;
  /* height: 100%; */
}

.container .finish .listUl .listLi .date {
  display: block;
  font-size: 25rpx;
  padding-left: 50rpx;

  /* margin-top: 50rpx; */
}

/* 字母换行 */
.container .finish .listUl .listLi .content {
  word-break: break-all;
}







/* 选框样式 */

/* 用于设置未选中时框的样式 */
checkbox .wx-checkbox-input {
  width: 30rpx;
  height: 30rpx;
  border-color: #fc606b;
  background-color: transparent;
  /* transition: background-color .2s; */
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  color: #6e6e6e;
  /* 这里也可以设置对钩的颜色 */
  background-color: #6e6e6e;
}